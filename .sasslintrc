{
  "options": {
  },
  "files": {
    "ignore": [
      "node_modules/**/*.*"
    ]
  },
  "rules": {
  "mixins-before-declarations": 0,
    "no-color-literals": 0,
    "property-sort-order": 0,
    "no-vendor-prefixes": 0,
    "nesting-depth": [
      1,
      {
        "max-depth": 2
      }
    ],
    "class-name-format": [
      1,
      {
        "convention": "hyphenatedbem"
      }
    ],
    "indentation": [
      1,
      {
        "size": "tab"
      }
    ]
  }
}
